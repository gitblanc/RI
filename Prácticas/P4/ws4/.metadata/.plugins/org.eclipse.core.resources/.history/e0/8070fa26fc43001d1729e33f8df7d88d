package uo.ri.cws.application.ui.cashier.action;

import console.Console;
import menu.Action;
import uo.ri.cws.application.business.BusinessException;
import uo.ri.cws.application.business.BusinessFactory;
import uo.ri.cws.application.business.mechanic.MechanicService;
import uo.ri.cws.application.business.mechanic.MechanicService.MechanicBLDto;

public class FindNotInvoicedWorkOrdersAction implements Action {

	@Override
	public void execute() throws BusinessException {
		MechanicBLDto mechanic = new MechanicBLDto();
		mechanic.dni = Console.readString("Client DNI ");
		MechanicService ms = BusinessFactory.forMechanicService();
		ms.findMechanicByDni(mechanic.dni);
		Console.println("\nClient's not invoiced work orders\n");  
	}

}